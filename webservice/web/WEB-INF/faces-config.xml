<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">

<managed-bean>
    <managed-bean-name>produtor</managed-bean-name>
    <managed-bean-class>br.unoesc.ws.configs.ProdutorController</managed-bean-class>

    <managed-bean-scope>session</managed-bean-scope></managed-bean>

<navigation-rule>
    <from-view-id>/cadProdutor.jsp</from-view-id>

    <navigation-case>
        <from-outcome>sucesso</from-outcome>
        <to-view-id>/sucesso.jsp</to-view-id>
    </navigation-case>

    <navigation-case>
        <from-outcome>falha</from-outcome>
        <to-view-id>/falha.jsp</to-view-id>
    </navigation-case>
</navigation-rule>
    <application>
        <el-resolver>br.unoesc.ws.configs.util.JsfCrudELResolver</el-resolver>
    </application>
    <navigation-rule>
        <navigation-case>
            <from-outcome>welcome</from-outcome>
            <to-view-id>/welcomeJSF.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>empresa</managed-bean-name>
        <managed-bean-class>br.unoesc.ws.configs.EmpresaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>empresaJpa</managed-bean-name>
        <managed-bean-class>br.unoesc.ws.configs.EmpresaJpaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>br.unoesc.ws.model.Empresa</converter-for-class>
        <converter-class>br.unoesc.ws.configs.EmpresaConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>empresa_create</from-outcome>
            <to-view-id>/empresa/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>empresa_list</from-outcome>
            <to-view-id>/empresa/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>empresa_edit</from-outcome>
            <to-view-id>/empresa/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>empresa_detail</from-outcome>
            <to-view-id>/empresa/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>cidade</managed-bean-name>
        <managed-bean-class>br.unoesc.ws.configs.CidadeController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>cidadeJpa</managed-bean-name>
        <managed-bean-class>br.unoesc.ws.configs.CidadeJpaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>br.unoesc.ws.model.Cidade</converter-for-class>
        <converter-class>br.unoesc.ws.configs.CidadeConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>cidade_create</from-outcome>
            <to-view-id>/cidade/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>cidade_list</from-outcome>
            <to-view-id>/cidade/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>cidade_edit</from-outcome>
            <to-view-id>/cidade/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>cidade_detail</from-outcome>
            <to-view-id>/cidade/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>estado</managed-bean-name>
        <managed-bean-class>br.unoesc.ws.configs.EstadoController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>estadoJpa</managed-bean-name>
        <managed-bean-class>br.unoesc.ws.configs.EstadoJpaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>br.unoesc.ws.model.Estado</converter-for-class>
        <converter-class>br.unoesc.ws.configs.EstadoConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>estado_create</from-outcome>
            <to-view-id>/estado/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>estado_list</from-outcome>
            <to-view-id>/estado/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>estado_edit</from-outcome>
            <to-view-id>/estado/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>estado_detail</from-outcome>
            <to-view-id>/estado/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>produtorJpa</managed-bean-name>
        <managed-bean-class>br.unoesc.ws.configs.ProdutorJpaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <converter>
        <converter-for-class>br.unoesc.ws.model.Produtor</converter-for-class>
        <converter-class>br.unoesc.ws.configs.ProdutorConverter</converter-class>
    </converter>
    <navigation-rule>
        <navigation-case>
            <from-outcome>produtor_create</from-outcome>
            <to-view-id>/produtor/New.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>produtor_list</from-outcome>
            <to-view-id>/produtor/List.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>produtor_edit</from-outcome>
            <to-view-id>/produtor/Edit.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>produtor_detail</from-outcome>
            <to-view-id>/produtor/Detail.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
